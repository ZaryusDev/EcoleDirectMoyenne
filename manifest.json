{
    "manifest_version": 3,
    "name": "EDM",
    "description":"Cette extension vous permet d'avoir votre moyenne générale ainsi que la moyenne de vos matières sur Ecole Direct.",
    "version": "0.1.5",
    "icons": {
        "16": "/images/icon16.png",
        "48": "/images/icon48.png",
        "128": "/images/icon128.png"
    },
    "content_scripts": [{
        "matches": ["https://www.ecoledirecte.com/*"],
        "js": ["main.js"],
        "css": []
    }]
}